{
  "name": "@yr/is-equal",
  "description": "Determine whether two objects are conceptually equal",
  "version": "2.0.1",
  "author": "Alexander Pope <alexander.pope@nrk.no>, Simen SÃ¦grov <simen.sagrov@nrk.no",
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "prettier": "^1.12.1",
    "renamer": "^0.6.1",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "mocha": "^5.1.1",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.3"
  },
  "main": "dist/index",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/YR/is-equal.git",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build-mjs && npm run build-cjs",
    "build-cjs": "tsc --module commonjs --target ES5",
    "build-mjs": "tsc --module es2015 --target ES2017 && renamer --regex --find '\\.js$' --replace '.mjs' 'dist/*.js'",
    "clean": "rimraf dist/",
    "lint": "prettier --list-different \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test mocha --require ts-node/register src/**.spec.ts --reporter spec"
  },
  "engines": {
    "node": ">=6"
  }
}
