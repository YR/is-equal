{
  "name": "@yr/is-equal",
  "description": "Determine whether two objects are conceptually equal",
  "version": "1.0.1",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
  },
  "devDependencies": {
    "buddy": "*",
    "buddy-plugin-babel": "6.5.2-2",
    "expect.js": "*",
    "mocha": "*",
    "mocha-phantomjs": "*"
  },
  "main": "src/index.js",
  "repository": "https://github.com/YR/is-equal.git",
  "license": "MIT",
  "scripts": {
    "prepublish": "buddy build",
    "test": "NODE_ENV=test mocha test/lib-test.js --reporter spec",
    "test-client": "buddy build && mocha-phantomjs test/test-runner.html"
  },
  "engines": {
    "node": ">=4"
  },
  "browser": "index.js",
  "build": {
    "targets": [
      {
        "input": "src",
        "output": ".",
        "modular": false
      },
      {
        "input": "src/index.js",
        "output": "test/lib.js",
        "boilerplate": true,
        "bootstrap": true
      }
    ]
  }
}
